#################### maxsim ####################

DirectorySlash Off

RewriteEngine on
RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule .* /$maxsim.php [L]

<FilesMatch "\.(php|phpt|txt|json|xml|yml|log|ini|env)$">
 Order Allow,Deny
 Deny from all
</FilesMatch>

<FilesMatch "(password|^\.)">
 Order Allow,Deny
 Deny from all
</FilesMatch>

<Files $maxsim.php>
 Order Allow,Deny
 Allow from all
</Files>

RedirectMatch 403 /\.git\.*

